// problem link : https://www.hackerrank.com/contests/eub-mujib-100-years-programming-contest-2020/challenges/sack-for-cost-minimization
// -------------------------------------------------------------------------------------------------------------------------------------

#include<bits/stdc++.h>
#define faster ios::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
#define ll long long int
using namespace std;

int main()
{
    faster
    int t ; cin >> t ;
    int j = 1;
    while (t--)
    {
        int arr[3];
        for (int i =0; i<3; i++)
        {
            cin >> arr[i];
        }
        sort (arr,arr+3);
        cout << "Case " << j << ": "<< arr[1] << endl ;
        j++;
    }
}


